<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.game.mp4">

    <uses-permission android:name="android.permission.INTERNET" />

    <queries>
        <package android:name="com.eg.android.AlipayGphone" />
    </queries>

    <application
        android:name="com.game.mp4.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CarOrder">
        <activity
            android:name="com.game.mp4.activity.ListActivity"
            android:exported="false" />
        <activity
            android:name="com.game.mp4.activity.PlayActivity"
            android:exported="false" />
        <activity
            android:name="com.game.mp4.activity.SignActivity"
            android:exported="true" />
        <activity
            android:name="com.game.mp4.activity.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.game.mp4.activity.MainActivity"
            android:exported="true" />
    </application>

</manifest>